@font-face {
  font-family: 'EntypoUni'; 
  src: url('entypo.eot'); 
  src: url('entypo.eot?#iefix') format('embedded-opentype'), url('entypo.ttf') format('truetype'), url('entypo.svg#entypo') format('svg'); 
  font-weight: normal; font-style: normal;
}

[class^="icon-"], [class*=" icon-"] {
  font-family: EntypoUni;
  font-style: normal;
  font-size: 14px;
  
  display: inline-block;
  width: 1em;
  text-align: center;
}

{{#glyphs}}
.{{css-prefix}}{{css}}:before { content: "\{{code}}"; }
{{/glyphs}}
